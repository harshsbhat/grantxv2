{
    "$schema": "https://turborepo.org/schema.json",
    "pipeline": {
      "dev": {
        "dependsOn": ["^dev"],
        "cache": false,
        "persistent": true
      },
      "build": {
        "dependsOn": ["^build"],
        "outputs": ["dist/**", ".next/**"]
      },
      "lint": {
        "dependsOn": ["^lint"],
        "outputs": []
      },
      "test": {
        "dependsOn": ["^test"],
        "outputs": []
      }
    }
  }
  